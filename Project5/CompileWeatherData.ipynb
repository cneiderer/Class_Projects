    "def map_au_code(code):    \n",
    "    pattern = '(\\w{2})*(\\+|\\-)*(\\w+\\:*\\d*)'\n",
    "    search = re.compile(pattern)\n",
    "    grps = search.findall(code)[0]\n",
    "    weather_str = ' '.join([au_map[grps[1]], au_map[grps[2]], au_map[grps[0]]]).strip()    \n",
    "    return weather_str"
   ]
